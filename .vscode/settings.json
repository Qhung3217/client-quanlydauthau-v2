{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.workingDirectories": [
    {
      "directory": "../src",
      "changeProcessCWD": true
    }
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#964de8",
    "activityBar.background": "#964de8",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#e99b56",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#964de8",
    "statusBar.background": "#7b20e2",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#964de8",
    "statusBarItem.remoteBackground": "#7b20e2",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#7b20e2",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7b20e299",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#7b20e2"
}
